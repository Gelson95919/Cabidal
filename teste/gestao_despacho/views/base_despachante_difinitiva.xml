<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <!--Este e Valido-->
         <record id="despachante_definitiva_act_window" model="ir.actions.act_window">
           <field name="name">Base Despachante Definitiva</field>
           <field name="res_model">despesa.despesa</field>
           <field name="view_type">form</field>
           <field name="view_mode">tree,form</field>
           <field eval="False" name="view_id"/>
           <field name="domain">[('type','=','despachante_definitiva')]</field>
           <field name="context">{'default_type': 'despachante_definitiva', 'type': 'despachante_definitiva'}</field>
           <field eval="False" name="view_id"/>
           <field name="help" type="html">
               <p class="oe_view_nocontent_create">
                 Criar um novo Base Despachante Definitiva.
               </p><p>
                 Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Base Despachante Definitiva)
                 para projetos internos, pessoais ou de clientes.
               </p>
           </field>
        </record>
        <record id="tree_despachante_definitiva_tree" model="ir.actions.act_window.view">
           <field eval="1" name="sequence"/>
           <field name="view_mode">tree</field>
           <field name="view_id" ref="gestao_compras.form_all_desp_tree_view"/>
           <field name="act_window_id" ref="despachante_definitiva_act_window"/>
        </record>
        <record id="form_despachante_definitiva_view2" model="ir.actions.act_window.view">
           <field eval="2" name="sequence"/>
           <field name="view_mode">form</field>
           <field name="view_id" ref="gestao_compras.form_all_docum_desp"/>
           <field name="act_window_id" ref="despachante_definitiva_act_window"/>
        </record>
        <menuitem action="despachante_definitiva_act_window" id="despachante_definitiva_menu" parent="gestao_despacho_menu" sequence="1"/>
    </data>
</odoo>