<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

         <record id="base_despesa_definitiva_act_window" model="ir.actions.act_window">
           <field name="name">Base Despesa Definitiva</field>
           <field name="res_model">despesa.despesa</field>
           <field name="view_type">form</field>
           <field name="view_mode">tree,form</field>
           <field eval="False" name="view_id"/>
           <field name="domain">[('type','=','base_despesa_definitiva')]</field>
           <field name="context">{'default_type': 'base_despesa_definitiva', 'type': 'base_despesa_definitiva'}</field>
           <field eval="False" name="view_id"/>
           <!--<field name="search_view_id" ref="view_account_invoice_filter"/>-->
           <field name="help" type="html">
               <p class="oe_view_nocontent_create">
                 Criar um novo Base Despesa Definitiva.
               </p><p>
                 Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Base Despesa Definitiva)
                 para projetos internos, pessoais ou de clientes.
               </p>
           </field>
        </record>
        <record id="tree_base_despesa_definitiva_tree" model="ir.actions.act_window.view">
           <field eval="1" name="sequence"/>
           <field name="view_mode">tree</field>
           <field name="view_id" ref="gestao_compras.form_all_desp_tree_view"/>
           <field name="act_window_id" ref="base_despesa_definitiva_act_window"/>
        </record>
        <record id="form_base_despesa_definitiva_view2" model="ir.actions.act_window.view">
           <field eval="2" name="sequence"/>
           <field name="view_mode">form</field>
           <field name="view_id" ref="gestao_compras.form_all_docum_desp"/>
           <field name="act_window_id" ref="base_despesa_definitiva_act_window"/>
        </record>
        <menuitem action="base_despesa_definitiva_act_window" id="base_despesa_definitiva_menu" parent="gestao_despacho_menu" sequence="1"/>



    </data>
</odoo>