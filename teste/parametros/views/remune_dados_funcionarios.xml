<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       <record id="situacoes_form" model="ir.ui.view">
           <field name="name">Situações</field>
           <field name="model">situacoes.situacoes</field>
           <field name="arch" type="xml">
               <form string="Situações">
                   <sheet>
                       <div>
                           <label for="title" string="Descrição"/>
                           <label for="title" string="_______" style="color:white"/>
                           <field name="name" style="width:400px"/>
                       </div>
                       <div>
                         <label for="title" string="Estado"/>
                         <field name="estado" widget="radio" options="{'horizontal': true}" nolabel="1"/>
                         <field name="baixa_prolong" /> <!-- attrs="{'readonly': [('tipo_recb','!=', 'baixaprolongada')]"-->
                       </div>
                   </sheet>
               </form>
           </field>
       </record>
       <record id="situacoes_tree" model="ir.ui.view">
           <field name="name">Situações</field>
           <field name="model">situacoes.situacoes</field>
           <field name="arch" type="xml">
               <tree string="Situações">
                   <field name="name"/>
               </tree>
           </field>
       </record>
       <record id="situacoes_act_window" model="ir.actions.act_window">
           <field name="name">Situações</field>
           <field name="type">ir.actions.act_window</field>
           <field name="res_model">situacoes.situacoes</field>
           <field name="view_mode">tree,form</field>
           <field name="help" type="html">
             <p class="oe_view_nocontent_create">
                Criar um novo Situações.
             </p><p>
                Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Situações) para projetos internos, pessoais ou de clientes.
             </p>
           </field>
       </record>
       <!-- This Menu Item must have a parent and an action -->
       <menuitem id="situacoes_mnenu" name="Situações" parent="fastgest_remuneracoe_dados_funcionario" action="situacoes_act_window" sequence="80"/>
