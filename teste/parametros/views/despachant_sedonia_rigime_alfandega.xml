<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

       <record id="regime_alfandega_form" model="ir.ui.view">
           <field name="name">Regime de Alfandega</field>
           <field name="model">regime.alfandega</field>
           <field name="arch" type="xml">
               <form string="Regime de Alfandega">
                   <sheet>
                       <group>
                           <field name="name"/>
                       </group>
                   </sheet>
               </form>
           </field>
       </record>
       <record id="regime_alfandega_tree" model="ir.ui.view">
           <field name="name">Regime de Alfandega</field>
           <field name="model">regime.alfandega</field>
           <field name="arch" type="xml">
               <tree string="Regime de Alfandega">
                   <field name="name"/>
               </tree>
           </field>
       </record>
       <record id="regime_alfandega_act_window" model="ir.actions.act_window">
           <field name="name">Regime de Alfandega</field>
           <field name="type">ir.actions.act_window</field>
           <field name="res_model">regime.alfandega</field>
           <field name="view_mode">tree,form</field>
           <field name="help" type="html">
             <p class="oe_view_nocontent_create">
               Criar um novo Regime de Alfandega.
             </p><p>
                Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Regime de Alfandega) para projetos internos, pessoais ou de clientes.
             </p>
           </field>
       </record>
       <!-- This Menu Item must have a parent and an action -->
       <menuitem id="regime_alfandega_menu" name="Regime de Alfandega" parent="fastgest_despachante_agencia" action="regime_alfandega_act_window" sequence="80"/>
    </data>
</odoo>