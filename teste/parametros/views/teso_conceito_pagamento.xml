<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <record id="conceito_pagamento_act_window" model="ir.actions.act_window">
           <field name="name">Conceitos Pagamento</field>
           <field name="res_model">compras.compras</field>
           <field name="view_type">form</field>
           <field name="view_mode">tree,form</field>
           <field eval="False" name="view_id"/>
           <field name="domain">[('type','=','conceitos_pagamento')]</field>
           <field name="context">{'default_type': 'conceitos_pagamento', 'type': 'conceitos_pagamento'}</field>
           <field eval="False" name="view_id"/>
           <!--<field name="search_view_id" ref="view_account_invoice_filter"/>-->
           <field name="help" type="html">
               <p class="oe_view_nocontent_create">
                 Criar um novo Conceitos Pagamento.
               </p><p>
                 Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Conceitos Pagamento)
                 para projetos internos, pessoais ou de clientes.
               </p>
           </field>
        </record>
        <record id="tree_conceitos_pagamento_tree" model="ir.actions.act_window.view">
           <field eval="1" name="sequence"/>
           <field name="view_mode">tree</field>
           <field name="view_id" ref="parametros.form_all_codcomp_tree_view"/>
           <field name="act_window_id" ref="conceito_pagamento_act_window"/>
        </record>
        <record id="form_conceito_pagamento_view2" model="ir.actions.act_window.view">
           <field eval="2" name="sequence"/>
           <field name="view_mode">form</field>
           <field name="view_id" ref="parametros.form_all_cod_com"/>
           <field name="act_window_id" ref="conceito_pagamento_act_window"/>
        </record>
        <menuitem action="conceito_pagamento_act_window" id="conceito_pagamento_menu" parent="fastgest_tesoura" sequence="1"/>
    </data>
</odoo>