<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <!--Este e Valido-->
         <record id="conceito_fatur_act_window" model="ir.actions.act_window">
           <field name="name">Conceito Faturação</field>
           <field name="res_model">produto.produto</field>
           <field name="view_type">form</field>
           <field name="view_mode">tree,form</field>
           <field eval="False" name="view_id"/>
           <field name="domain">[('type','=','2')]</field>
           <field name="context">{'default_type': '2', 'type': '2'}</field>
           <field eval="False" name="view_id"/>
           <field name="help" type="html">
               <p class="oe_view_nocontent_create">
                 Criar um novo Conceito Faturação.
               </p><p>
                 Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Conceito Faturação)
                 para projetos internos, pessoais ou de clientes.
               </p>
           </field>
        </record>
        <record id="tree_conceito_fat_tree" model="ir.actions.act_window.view">
           <field eval="1" name="sequence"/>
           <field name="view_mode">tree</field>
           <field name="view_id" ref="parametros.form_all_prod_tree_view"/>
           <field name="act_window_id" ref="conceito_fatur_act_window"/>
        </record>
        <record id="form_conceito_fat_view2" model="ir.actions.act_window.view">
           <field eval="2" name="sequence"/>
           <field name="view_mode">form</field>
           <field name="view_id" ref="parametros.form_all_produt"/>
           <field name="act_window_id" ref="conceito_fatur_act_window"/>
        </record>
        <menuitem action="conceito_fatur_act_window" name="Conceito Faturação"  id="conceito_fat_menu" parent="fastgest_age_maritima" sequence="1"/>
    </data>
</odoo>