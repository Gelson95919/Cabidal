        <menuitem id="contentor_menu" name="Contentor" parent="parametros.fastgest_age_maritima" action="contentor_act_window" sequence=""/>
        <record id="pertence_form" model="ir.ui.view">
            <field name="name">Pertences</field>
            <field name="model">pertences</field>
            <field name="arch" type="xml">
                <form string="Pertences">
                    <sheet>
                        <div><label for="title" string=""/> </div>
                        <div>
                            <label for="title" string="Referência Pertences"/>
                            <field name="ref_pertences" style="width:100px"/>
                            <label for="title" string="" style="width:260px"/>
                            <label for="title" string="Nº Pertences"/>
                            <field name="numero_trans" style="width:100px"/>
                        </div>
                        <div><label for="title" style = "color:blue; format:bold" string="Destinatário______________________________________________________________________________________________________________________"/> </div>
                        <div>
                            <label for="title" string="Nome"/>
                            <field name="name" style="width:260px"/>
                            <label for="title" string="" style="width:260px"/>
                            <label for="title" string="NIF"/>
                            <field name="nif" style="width:260px"/>
                        </div>
                        <div><label for="title" string="Endereço"/></div>
                        <div>
                            <field name="endereco"/>
                        </div>
                        <div><label for="title" string=""/> </div>
                        <div><label for="title" style = "color:blue; format:bold" string="Notificar a____________________________________________________________________________________________________________________"/> </div>
                        <div>
                            <label for="title" string="Nome"/>
                            <field name="nome" style="width:260px"/>
                        </div>
                        <div><label for="title" string="Endereço"/></div>
                        <div>
                            <field name="endereco_notif"/>
                        </div>
                        <div>
                            <field name="contentor_ids" style="width:1000px">
                                <tree string="Contentores" editable="bottom" >
                                    <field name="numero_sequencia"/>
                                    <field name="referencia"/>
                                    <field name="tipo"/>
                                </tree>
                            </field>
                        </div>
                        <div><label for="title" string=""/> </div>
                        <div><label for="title" style = "color:blue; format:bold" string="Cargas_________________________________________________________________________________________________________________________"/> </div>
                        <div>
                            <label for="title" string="Embalagem"/>
                            <field name="embalagem" style="width:260px"/>
                            <label for="title" string="" style="width:260px"/>
                            <label for="title" string="Volumes"/>
                            <field name="volumes" style="width:260px"/>
                        </div>
                        <div>
                            <label for="title" string="Peso(Kg)"/>
                            <field name="peso" style="width:260px"/>
                            <label for="title" string="" style="width:260px"/>
                            <label for="title" string="Cubicagem(M3)"/>
                            <field name="cubicagem" style="width:210px"/>
                        </div>
                        <div><label for="title" string="Marca"/></div>
                        <div>
                            <field name="marca"/>
                        </div>
                        <div><label for="title" string="Descrição"/></div>
                        <div>
                            <field name="descr_carga"/>
                        </div>
                        <div><label for="title" style = "color:blue; format:bold" string="Informação Adicional_____________________________________________________________________________________________________________"/> </div>
                        <div>
                            <field name="informacao"/>
                        </div>
                        <div>
                            <label for="title" string="Valor Frete"/>
                            <field name="valor_frete" style="width:260px"/>
                            <label for="title" string="" style="width:260px"/>
                            <label for="title" string="Moeda"/>
                            <field name="moeda_id" style="width:260px"/>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="pertence_act_window" model="ir.actions.act_window">
