        <record id="contentor_form" model="ir.ui.view">
            <field name="name">Contentor</field>
            <field name="model">contentor</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <div><label for="title" string=""/> </div>
                        <div>
                            <label for="title" string="Titulo Transporte"/>
                            <field name="titulo_transporte" style="width:460px"/>
                        </div>
                        <div><label for="title" string="Contentor" style = "color:blue; format:bold"/></div>
                        <div>
                            <label for="title" string="Numero Sequencia"/>
                            <field name="numero_sequencia" style="width:260px"/>
                            <label for="title" string="" style="width:200px"/>
                            <label for="title" string="Referência"/>
                            <field name="referencia" style="width:260px"/>
                        </div>
                        <div>
                            <label for="title" string="Tipo"  style="width:120px"/>
                            <field name="tipo" style="width:260px"/>
                            <label for="title" string="" style="width:200px"/>
                            <label for="title" string="Indicador"/>
                            <field name="indicador" style="width:260px"/>
                        </div>
                        <div>

                        </div>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="contentor_act_window" model="ir.actions.act_window">
            <field name="name">Contentor</field>
            <field name="type">ir.actions.act_window</field>
