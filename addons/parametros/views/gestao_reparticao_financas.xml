<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="reparticao_financa_form" model="ir.ui.view">
            <field name="name">Repartição Finanças</field>
            <field name="model">reparticao.financa</field>
            <field name="arch" type="xml">
                <form string="Repartição Finanças">
                    <sheet>
                        <div><label for="title" string="" style="width:20px"/> </div>
                        <div>
                            <label for="title" string="Codigo"/>
                            <field name="codigo" style="width:100px"/>
                        </div>
                        <div>
                            <label for="title" string="Nome"/>
                            <field name="name" style="width:400px"/>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="reparticao_financa_tree" model="ir.ui.view">
            <field name="name">Repartição Finanças</field>
            <field name="model">reparticao.financa</field>
            <field name="arch" type="xml">
                <tree string="Repartição Finanças">
                    <field name="codigo"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
