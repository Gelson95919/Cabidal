       <record id="frete_cabotagem_tree" model="ir.ui.view">
           <field name="name">Frete Cabotagem</field>
           <field name="model">frete.cabotagem</field>
           <field name="arch" type="xml">
               <tree string="Frete Cabotagem">

               </tree>
           </field>
       </record>
       <record id="frete_cabotagem_act_window" model="ir.actions.act_window">
           <field name="name">Frete Cabotagem</field>
           <field name="type">ir.actions.act_window</field>
           <field name="res_model">frete.cabotagem</field>
           <field name="view_mode">tree,form</field>
           <field name="help" type="html">
             <p class="oe_view_nocontent_create">
               Criar um novo Frete Cabotagem.
             </p><p>
                Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Frete Cabotagem) para projetos internos, pessoais ou de clientes.
             </p>
           </field>
       </record>
       <!-- This Menu Item must have a parent and an action -->
       <menuitem id="frete_cabotagem_menu" name="Frete Cabotagem" parent="fastgest_age_maritima" action="frete_cabotagem_act_window" sequence="80"/>
    </data>
