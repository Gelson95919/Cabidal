<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="tipo_imobil_form" model="ir.ui.view">
            <field name="name">Tipo Imobilizado</field>
            <field name="model">tipo.imobilizado</field>
            <field name="arch" type="xml">
                <form string="Tipo Imobilizado">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="tipo_imobil_trre" model="ir.ui.view">
            <field name="name">Tipo Imobilizado</field>
            <field name="model">tipo.imobilizado</field>
            <field name="arch" type="xml">
                <tree string="Tipo Imobilizado">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="tipo_imobil_act_window" model="ir.actions.act_window">
            <field name="name">Tipo Imobilizado</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">tipo.imobilizado</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Criar um novo Tipo de Imobilizado.
              </p><p>
                Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Tipo de Imobilizado) para projetos internos, pessoais ou de clientes.
              </p>
            </field>
        </record>
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="tipo_imobil_meno" name="Tipo Imobilizado" parent="fastgest_invetimento" action="tipo_imobil_act_window" sequence="80"/>
        <!--<menuitem name="IVA" id="menu_iva_invent" parent="fastgest_invetimento" action="parametros.action_window_iva"/>-->
    </data>
</odoo>