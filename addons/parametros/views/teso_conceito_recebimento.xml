<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

         <record id="conceito_recebmento_act_window" model="ir.actions.act_window">
           <field name="name">Conceitos Recebimento</field>
           <field name="res_model">produto.produto</field>
           <field name="view_type">form</field>
           <field name="view_mode">tree,form</field>
           <field eval="False" name="view_id"/>
           <field name="domain">[('type','=','3')]</field>
           <field name="context">{'default_type': '3', 'type': '3'}</field>
           <field eval="False" name="view_id"/>
           <!--<field name="search_view_id" ref="view_account_invoice_filter"/>-->
           <field name="help" type="html">
               <p class="oe_view_nocontent_create">
                 Criar um novo Conceitos Recebimento.
               </p><p>
                 Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Conceitos Recebimento)
                 para projetos internos, pessoais ou de clientes.
               </p>
           </field>
        </record>
        <record id="tree_conceitos_recebimento_tree" model="ir.actions.act_window.view">
           <field eval="1" name="sequence"/>
           <field name="view_mode">tree</field>
           <field name="view_id" ref="parametros.form_all_prod_tree_view"/>
           <field name="act_window_id" ref="conceito_recebmento_act_window"/>
        </record>
        <record id="form_conceito_recebimento_view2" model="ir.actions.act_window.view">
           <field eval="2" name="sequence"/>
           <field name="view_mode">form</field>
           <field name="view_id" ref="parametros.form_all_produt"/>
           <field name="act_window_id" ref="conceito_recebmento_act_window"/>
        </record>
        <menuitem action="conceito_recebmento_act_window" id="conceito_recebimento_menu" parent="fastgest_tesoura" sequence="1"/>

    </data>
</odoo>