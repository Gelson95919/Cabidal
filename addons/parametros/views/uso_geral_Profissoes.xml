<odoo>
    <data>
        <record model="ir.ui.view" id="form_all_prof">
            <field name="name">fastgest Profissões</field>
            <field name="model">profissoes.profissoes</field>
            <field name="arch" type="xml">
                <form string="Cargos">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                            <group>
                               <field name="codigo"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="form_prof_tree_view" model="ir.ui.view">
            <field name="name">iva.tree.view</field>
            <field name="model">profissoes.profissoes</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="codigo"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="cod_prof_sequ" model="ir.sequence">
            <field name="name">Professões</field>
            <field name="code">profissoes</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="compan_id" eval="False"/>
        </record>
        <record model="ir.actions.act_window" id="parametros.action_window_prof">
            <field name="name">Profissões</field>
            <field name="res_model">profissoes.profissoes</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('type','=','professoes')]</field>
            <field name="context">{'default_type': 'professoes', 'type': 'professoes'}</field>
            <field eval="False" name="view_id"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Criar um novo Profissões.
                </p>
                <p>
                    Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Profissões) para
                    projetos internos, pessoais ou de clientes.
                </p>
            </field>
        </record>
        <menuitem name="Profissões " id="parametros.menu_prof_list" parent="parametros.menu_Uso_Geral"
                  action="parametros.action_window_prof"/>
    </data>
</odoo>
