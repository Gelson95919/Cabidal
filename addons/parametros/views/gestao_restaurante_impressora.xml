<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

       <record id="impressora_form" model="ir.ui.view">
           <field name="name">Impressoras</field>
           <field name="model">impressoras.impressoras</field>
           <field name="arch" type="xml">
               <form string="Impressoras">
                   <sheet>
                     <group>
                         <group>
                           <field name="name"/>
                         </group>
                         <group>
                             <field name="impressor"/>
                         </group>
                     </group>
                   </sheet>
               </form>
           </field>
       </record>
       <record id="impressora_tree" model="ir.ui.view">
           <field name="name">Impressoras</field>
           <field name="model">impressoras.impressoras</field>
           <field name="arch" type="xml">
               <tree string="Impressoras">
                   <field name="name"/>
               </tree>
           </field>
       </record>
       <record id="impressora_act_window" model="ir.actions.act_window">
           <field name="name">Impressoras</field>
           <field name="type">ir.actions.act_window</field>
           <field name="res_model">impressoras.impressoras</field>
           <field name="view_mode">tree,form</field>
           <field name="help" type="html">
             <p class="oe_view_nocontent_create">
               Realizar um novo Impressoras.
              </p><p>
                Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Impressoras) para projetos internos, pessoais ou de clientes.
              </p>
           </field>
       </record>
       <!-- This Menu Item must have a parent and an action -->
       <menuitem id="impressora_menu" name="Impressoras" parent="fastgest_restaurante" action="impressora_act_window" sequence="10"/>
    </data>
</odoo>