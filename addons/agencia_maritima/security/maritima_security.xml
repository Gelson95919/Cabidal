<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Data records go here -->
        <record id="group_maitima_user" model="res.groups">
            <field name="name">User Cabosys Maritima</field>
            <field name="category_id" ref="base.module_category_maritima"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        <record id="group_maritima_manager" model="res.groups">
            <field name="name">Manager Maritima</field>
            <field name="category_id" ref="base.module_category_maritima"/>
            <field name="implied_ids" eval="[(4, ref('group_maitima_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
    </data>
</odoo>