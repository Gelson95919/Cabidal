<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="schoolem_evaluation_form" model="ir.ui.view">
            <field name="name">schoolem evaluation</field>
            <field name="model">schoolem.evaluation</field>
            <field name="arch" type="xml">
                <form string="schoolem evaluation">
                    <sheet>
                        <group>
                           <group>
                               <field name="name"/>
                               <field name="aca_id"/>
                               <field name="periode_id"/>
                               <field name="examen_id"/>
                          </group>
                          <group>
                              <field name="salle_de_classe_id"/>
                              <field name="cours_id"  context="{'evaluation_id': active_id, 'test': 1}" on_change="on_change_cours_id(examen_id,salle_de_classe_id,cours_id,aca_id)"/>
                          </group>
                         <notebook>
                            <page string="Inserer des notes">
                                <field nolabel="1" name="note_ids" context="{'evaluation_id': active_id}"/>
                            </page>
                         </notebook>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="schoolemevaluation_act_w" model="ir.actions.act_window">
            <field name="name">schoolem evaluation</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">schoolem.evaluation</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                <!-- Add Text Here -->
                  Criar
              </p><p>
                <!-- More details about what a user can do with this object will be OK -->
              </p>
            </field>
        </record>
        <!-- This Menu Item will appear in the Upper bar, That's why It needs NO parent or action -->
        <menuitem id="schoolem_evaluation_root" name="schoolem evaluation" sequence="80"/>
        <!-- This Menu Item Must have a parent -->
        <menuitem id="schoolem_evaluation_categ" name="Menu1" parent="schoolem_evaluation_root" sequence="80"/>
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="schoolem_evaluation_ID_categ" name="Eccola" parent="schoolem_evaluation_categ" action="schoolemevaluation_act_w" sequence="80"/>

