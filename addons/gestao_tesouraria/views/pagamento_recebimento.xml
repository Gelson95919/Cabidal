        <record id="pagamento_recebimento_tree" model="ir.ui.view">
            <field name="name">Pagamento/Recebimento/Movimento</field>
            <field name="model">pagamento.recebimento</field>
            <field name="arch" type="xml">
                <tree string="Pagamento/Recebimento/Movimento">
                    <field name="cheque" invisible="context.get ('type_docum')!= 'movimento'"/>
                    <field name="cod_cliente" invisible="context.get ('type_docum') != 'recebimento'"/>
                    <field name="id" invisible="context.get ('type_docum') != 'pagamento'"/>
                    <field name="cod_terceiro" invisible="context.get ('type_docum') != 'pagamento'"/>
                    <field name="nome_cliente" invisible="context.get ('type_docum') != 'recebimento'"/>
                    <field name="nome_terceiro" invisible="context.get ('type_docum')!= 'pagamento'"/>
                    <field name="montante_receb" invisible="context.get ('type_docum')!= 'recebimento'"/>
                    <field name="montante_pago" invisible="context.get ('type_docum')!= 'pagamento'"/>
                    <field name="nossa_conta_transfe" invisible="context.get ('type_docum')!= 'movimento'"/>
                    <field name="conta_destino" invisible="context.get ('type_docum')!= 'movimento'"/>
                    <field name="montante_mov" invisible="context.get ('type_docum')!= 'movimento'"/>
                    <field name="anulado" invisible="context.get ('type_docum')!= 'movimento'"/>
                    <field name="date"/>
                    <field name="detalhes"/>
                </tree>
            </field>
        </record>
