                     <group expand="0" string="Group By">
                         <filter name="group_descricao" string="Período"
                                 context="{'group_by':'descricao'}"
                                 help="Group Período Wise"/>
                     </group>
                 </search>
             </field>
         </record>-->
        <record model="ir.actions.act_window" id="action_window_orcamento">
            <field name="name">Orçamento Tesouraria</field>
            <field name="res_model">orcamenteso.orcamenteso</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'search_default_group_descricao': 1}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Realizar um novo Orçamento Tesouraria.
                </p>
                <p>
                    Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Orçamento) para
                    projetos internos, pessoais ou de clientes.
                </p>
            </field>
        </record>
        <menuitem name=" Orçamento Tesouraria " id="menu_orcam_list" parent="gestao_tesouraria.menu_1"
                  action="action_window_orcamento"/>

    </data>
</odoo>
