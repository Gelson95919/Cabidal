        <!-- Data records go here -->
        <record id="group_investimento_user" model="res.groups">
            <field name="name">User Cabosys Investimnto</field>
            <field name="category_id" ref="base.module_category_investimento"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        <record id="group_investimento_manager" model="res.groups">
            <field name="name">Manager Investimento</field>
