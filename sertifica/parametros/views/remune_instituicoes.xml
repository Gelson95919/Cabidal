<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="reeposicao_financas_form" model="ir.ui.view">
            <field name="name">Reposição Finanças</field>
            <field name="model">reposicao.financas</field>
            <field name="arch" type="xml">
                <form string="Reposição Finanças">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="numero"/>
                            </group>
                            <group>
                                <field name="terceiro_id"/>
                                <field name="cod_comp_id"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="reeposicao_financas_tree" model="ir.ui.view">
            <field name="name">Reposição Finanças</field>
            <field name="model">reposicao.financas</field>
            <field name="arch" type="xml">
                <tree string="Reposição Finanças">
                    <field name="name"/>
                    <field name="numero"/>
                    <field name="terceiro_id"/>
                    <field name="cod_comp_id"/>
                </tree>
            </field>
        </record>

        <record id="reeposicao_financas_act_window" model="ir.actions.act_window">
            <field name="name">Reposição Finanças</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">reposicao.financas</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Criar um novo Reposição Finanças.
                </p>
                <p>
                    Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Reposição Finanças)
                    para projetos internos, pessoais ou de clientes.
                </p>
            </field>
        </record>
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="reposicao_financas_menu" name="Reposição Finanças" parent="fastgest_remuneracoe_instetuicao"
                  action="reeposicao_financas_act_window" sequence="80"/>
