<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

       <record id="classe_navio_form" model="ir.ui.view">
           <field name="name">Classe Navio</field>
           <field name="model">classe.navio</field>
           <field name="arch" type="xml">
               <form string="Classe Navio">
                   <sheet>
                       <group>
                           <field name="name"/>
                       </group>
                   </sheet>
               </form>
           </field>
       </record>
       <record id="classe_navio_tree" model="ir.ui.view">
           <field name="name">Classe Navio</field>
           <field name="model">classe.navio</field>
           <field name="arch" type="xml">
               <tree string="Classe Navio">
                   <field name="name"/>
               </tree>
           </field>
       </record>
       <record id="classe_navio_act_window" model="ir.actions.act_window">
           <field name="name">Classe Navio</field>
           <field name="type">ir.actions.act_window</field>
           <field name="res_model">classe.navio</field>
           <field name="view_mode">tree,form</field>
           <field name="help" type="html">
             <p class="oe_view_nocontent_create">
               Criar um novo Classe Navio.
             </p><p>
                Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Classe Navio) para projetos internos, pessoais ou de clientes.
             </p>
           </field>
       </record>
       <!-- This Menu Item must have a parent and an action -->
       <menuitem id="classe_navio_menu" name="Classe Navio" parent="fastgest_age_maritima" action="classe_navio_act_window" sequence="80"/>
    </data>
</odoo>