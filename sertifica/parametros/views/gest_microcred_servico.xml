<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <!--Este e Valido-->
         <record id="servic_act_window" model="ir.actions.act_window">
           <field name="name">Serviços</field>
           <field name="res_model">produto.produto</field>
           <field name="view_type">form</field>
           <field name="view_mode">tree,form</field>
           <field eval="False" name="view_id"/>
           <field name="domain">[('type','=','4')]</field>
           <field name="context">{'default_type': '4', 'type': '4'}</field>
           <field eval="False" name="view_id"/>
           <field name="help" type="html">
               <p class="oe_view_nocontent_create">
                 Criar um novo Serviços.
               </p><p>
                 Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Serviços)
                 para projetos internos, pessoais ou de clientes.
               </p>
           </field>
        </record>
        <record id="tree_servic_tree" model="ir.actions.act_window.view">
           <field eval="1" name="sequence"/>
           <field name="view_mode">tree</field>
           <field name="view_id" ref="parametros.form_all_prod_tree_view"/>
           <field name="act_window_id" ref="servic_act_window"/>
        </record>
        <record id="servic_view2" model="ir.actions.act_window.view">
           <field eval="2" name="sequence"/>
           <field name="view_mode">form</field>
           <field name="view_id" ref="parametros.form_all_produt"/>
           <field name="act_window_id" ref="servic_act_window"/>
        </record>
        <menuitem action="servic_act_window" name="Serviços"  id="servic_menu" parent="fastgest_gest_microcred_tabelas" sequence="1"/>
    </data>
</odoo>