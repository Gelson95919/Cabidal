<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

       <record id="combustivel_form" model="ir.ui.view">
           <field name="name">Combustivel</field>
           <field name="model">combustivel.combustivel</field>
           <field name="arch" type="xml">
               <form string="Combustivel">
                   <sheet>
                       <group>
                           <field name="name"/>
                       </group>
                   </sheet>
               </form>
           </field>
       </record>
       <record id="combustivel_tree" model="ir.ui.view">
           <field name="name">Combustivel</field>
           <field name="model">combustivel.combustivel</field>
           <field name="arch" type="xml">
               <tree string="Combustivel">
                   <field name="name"/>
               </tree>
           </field>
       </record>
       <record id="combustivel_act_window" model="ir.actions.act_window">
           <field name="name">Combustivel</field>
           <field name="type">ir.actions.act_window</field>
           <field name="res_model">combustivel.combustivel</field>
           <field name="view_mode">tree,form</field>
           <field name="help" type="html">
             <p class="oe_view_nocontent_create">
               Criar um novo Combustivel.
             </p><p>
                Organize suas atividades (planeje tarefas, acompanhe problemas, planilhas de Combustivel) para projetos internos, pessoais ou de clientes.
             </p>
           </field>
       </record>
       <!-- This Menu Item must have a parent and an action -->
       <menuitem id="combustivel_menu" name="Combustivel" parent="fastgest_age_maritima" action="combustivel_act_window" sequence="80"/>
    </data>
</odoo>